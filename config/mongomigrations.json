{
	"config": {
		"local": {
			"paths": [
				"{PROJECT}/mm.json"
			]
		},
		"global": {
			"paths": [
				"{USER}/mm.json",
				"{USER}/mongo-migrations.json"
			]
		}
	},
	"debug": {
		"log": {
			"path": "./logs/mm.log",
			"level": "DEBUG"
		}
	},
	"defaults": {
		"migrations" : {
			"path" : "{PROJECT}/db/migrations",
			"entries" : "mm_migration_log"
		}
	},
	"defer" : true,
	"dependencies": {
		"namespaces": [],
		"classes": [
			"Svhq/Core/Dependencies/Debug",
			"Svhq/Core/Dependencies/Mongo",
			"Svhq/Core/Dependencies/Resource"
		]
	},
	"environment": {
		"src": "./src"
	},
	"mongo": {
		"database" : "MongoMigrations",
		"host" : "127.0.0.1",
		"port" : "27017"
	}
}